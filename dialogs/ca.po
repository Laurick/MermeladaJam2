#: dialogs/intro.dialogue
msgctxt "dialogue"
msgid "Protagonist"
msgstr "Protagonista"

#: dialogs/intro.dialogue dialogs/scene_two.dialogue
msgctxt "dialogue"
msgid "Jim"
msgstr "Jim"

#: dialogs/intro.dialogue dialogs/scene_two.dialogue
msgctxt "dialogue"
msgid "Joe"
